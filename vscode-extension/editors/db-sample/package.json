    "@puredit/codemirror-typescript": "*",
    "@puredit/editor-interface": "*",
    "@puredit/projections": "*",
    "@puredit/simple-projection": "*",
    "@puredit/utils": "*",
    "@replit/codemirror-indentation-markers": "6",
